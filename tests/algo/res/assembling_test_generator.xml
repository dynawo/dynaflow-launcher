    <connection var1="UpPu_value" var2="@NAME@_Upu_value"/>
  </macroConnection>
  <macroConnection id="SVCToGenerator" index1="true">
    <connection var1="level_value" var2="reactivePowerControlLoop_level"/>
    <connection var1="blocker_@INDEX@_value" var2="generator_blocker"/>
    <connection var1="QStator_@INDEX@_value" var2="generator_QStator"/>
  </macroConnection>
  <singleAssociation id="MESURE_MODELE_VL">
    <bus voltageLevel="VL"/>
  </singleAssociation>
  <singleAssociation id="GeneratorId">
    <generator name="00"/>
    <generator name="01"/>
    <generator name="02"/>
  </singleAssociation>
  <dynamicAutomaton id="GeneratorAutomaton" lib="DYNModelSecondaryVoltageControlSimplified">
    <macroConnect macroConnection="SVCToUMeasurement" id="MESURE_MODELE_VL"/>
    <macroConnect macroConnection="SVCToGenerator" id="GeneratorId"/>
  </dynamicAutomaton>
</assembling>
